<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.myapplication6.ui.address.AddressFragment"
        android:label="Address"
        tools:layout="@layout/fragment_address" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.myapplication6.ui.gallery.GalleryFragment"
        android:label="Gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.myapplication6.ui.matching.MatchingFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_matching" />
    <fragment
        android:id="@+id/navigation_detail"
        android:name="com.example.myapplication6.ui.address.DetailFragment"
        android:label="Details"
        tools:layout="@layout/fragment_detail" />
</navigation>